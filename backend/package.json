{
  "name": "cute.homepage_backend",
  "version": "0.1.325",
  "description": "This is a small CMS for managing your portfolio via the Telegram",
  "main": "src/server.js",
  "bin": "dist/src/server.js",
  "scripts": {
    "start-ts": "ts-node src/server.ts",
    "start": "node dist/src/server.js",
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "db:push": "dotenv -c -- drizzle-kit push",
    "generate": "dotenv -c -- drizzle-kit generate",
    "migrate": "dotenv -c -- ts-node src/db/migrate.ts",
    "studio": "dotenv -c -- drizzle-kit studio",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write ."
  },
  "keywords": [
    "CMS",
    "Telegram",
    "Portfolio",
    "Ducheved"
  ],
  "author": "Ducheved",
  "license": "Mozilla Public License Version 2.0",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.620.1",
    "@eslint/js": "^9.8.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/websocket": "^10.0.1",
    "@lexical/code": "^0.14.5",
    "@lexical/html": "^0.14.5",
    "@lexical/link": "^0.14.5",
    "@lexical/list": "^0.14.5",
    "@lexical/markdown": "^0.14.5",
    "@lexical/react": "^0.14.5",
    "@lexical/rich-text": "^0.14.5",
    "@lexical/selection": "^0.14.5",
    "@lexical/table": "^0.14.5",
    "@lexical/utils": "^0.14.5",
    "@lucia-auth/adapter-drizzle": "^1.0.7",
    "@node-rs/argon2": "^1.8.3",
    "@node-rs/bcrypt": "^1.10.4",
    "@smithy/node-http-handler": "^3.1.4",
    "@types/katex": "^0.16.7",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^22.0.0",
    "@types/pg": "^8.11.6",
    "@types/ws": "^8.5.12",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "autoprefixer": "^10.4.19",
    "cva": "1.0.0-beta.1",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.1.3",
