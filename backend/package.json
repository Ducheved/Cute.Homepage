    "katex": "^0.16.11",
    "lexical": "^0.14.5",
    "lucia": "^3.2.0",
    "mime-types": "^2.1.35",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.1.4",
    "pg": "^8.12.0",
